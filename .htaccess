DirectoryIndex index.php
<Files ~ "^\.(.*)$">
    Order allow,deny
    Deny from all
    Satisfy all
</Files>
<IfModule mod_rewrite.c>
    RewriteEngine on

    # line for check
    RewriteRule checkmodrewriteitdc.php index.php?mode=mr [NC]
</IfModule>

ErrorDocument 404 /check/index.php?mode=error
ErrorDocument 403 /check/index.php?mode=error
